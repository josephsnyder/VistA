import sys
sys.path = ['${VISTA_SOURCE_DIR}/Python/vista'] + sys.path

from OSEHRAHelper import ConnectToMUMPS,PROMPT

VistA=ConnectToMUMPS("${logfile}","${VISTA_CACHE_INSTANCE}","${VISTA_CACHE_NAMESPACE}")
if ('${VISTA_CACHE_USERNAME}' and '${VISTA_CACHE_PASSWORD}'):
  VistA.login('${VISTA_CACHE_USERNAME}','${VISTA_CACHE_PASSWORD}')
if VistA.type=='cache':
  try:
    VistA.ZN('${VISTA_CACHE_NAMESPACE}')
  except IndexError,no_namechange:
    pass
VistA.wait(PROMPT)
VistA.write('D ^%GI')
if VistA.type=='GTM':
  VistA.wait('Input device: <terminal>:')
  VistA.write('${GlobalImportFile}')
  VistA.wait('OK')
  VistA.write('Y')
  VistA.wait(PROMPT, ${TEST_VISTA_FRESH_ROUTINE_IMPORT_TIMEOUT})
  VistA.write('H')
else:
  VistA.wait('Device:')
  #set the device to be the .GO file
  VistA.write('${GlobalImportFile}')
  VistA.wait('Parameters')
  #just use default
  VistA.write('')
  VistA.wait_re('Input [oO]ption:')
  VistA.write('A')
  VistA.wait(PROMPT, ${TEST_VISTA_FRESH_ROUTINE_IMPORT_TIMEOUT})
  VistA.write('h')
